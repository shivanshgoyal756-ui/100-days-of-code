#include <stdio.h>

int main() {
   int num;
   printf("Enter the two numbers:");
   scanf("%d",&num);
   int sum=0;
   for(int i=1;i<num;++i)
   {
       if(num%i==0)
       sum=sum+i;
   }
   if(sum==num)
   printf("The number is a perfect number");
   else
   printf("The number is not perfect");
    return 0;
}
