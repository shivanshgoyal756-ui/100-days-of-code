#include <stdio.h>


int main() {
   int n=0;
   int s=0;
   printf("Enter the number of days");
   scanf("%d",&n);
   if(n<=5)
   {
       s=n*2;
   }
   if(n>5&&n<=10)
   {
       s=10;
       n=n-5;
       s=s+(n*4);
   }
   if(n>10&&n<=30)
   {
       s=30;
       n=n-10;
       s=s+(s*6);
   }
   if(n>30)
   {
       printf("Membership cancel");
   }
   if(n<=30)
   {
       printf("The amount to be paid is :%d",s);
   }
   return 0;

   }
   
