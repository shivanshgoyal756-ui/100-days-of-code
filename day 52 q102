#include <stdio.h>

int main() {
   int num;
   int target;
   printf("Enter how many number are to be inputed");
   scanf("%d",&num);
   int a[num];
   printf("what is the target num");
   scanf("%d",&target);
   printf("\nEnter the array numbers");
   for(int i=0;i<num;++i)
   {
       scanf("%d",&a[i]);
   }
   int c=0;
   for(int i=0;i<num;++i)
   {
       if(a[i]>=target)
       {
           printf("\n%d",i+1);
           c=1;
           break;
       }
   }
   if(c==0)
   printf("\n-1");
    return 0;
}
