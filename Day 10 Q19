#include <stdio.h>


int main() {
    int a,b,c;
    printf("Enter the three sides");
    scanf("%d%d%d",&a,&b,&c);
    if(a==b&&b==c&&a==c)
    printf("The triangle is an equilateral triangle");
    else
    {
        if(a==b||a==c||b==c)
        {
             printf("The triangle is an Isosceles triangle");
        }
        else
        printf("The triangle is an Scalene triangle");
        
    }
    return 0;
}
