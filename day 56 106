#include <stdio.h>

int main() {
   int num;
   printf("Enter how many number are to be inputed");
   scanf("%d",&num);
   int a[num];
   int sum1=0;
   printf("\nEnter the array numbers");
   for(int i=0;i<num;++i)
   {
       scanf("%d",&a[i]);
   }
   int c=0;
   for(int i=0;i<num;++i)
   {
      for(int j=i;j<num;++j)
      {
          if(a[i]<a[j])
          {
           a[i]=a[j];
           c=1;
           break;
          }
          
      }
     if(c==0)
     a[i]=-1;
     c=0;
   }
   for(int i=0;i<num;++i)
   {
       printf("\n%d",a[i]);
   }
    return 0;
}
