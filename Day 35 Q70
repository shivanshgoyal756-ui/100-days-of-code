#include <stdio.h>

  int main() {
      int num;
      printf("Enter how many number are to be input 1st");
     scanf("%d",&num);
    int n[num];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
    int k;
    printf("Enter the value of k");
    scanf("%d",&k);
    for(int a=(num-k);a<num;++a)
    {
      printf("%d",n[a]);
    }
     
     for (int b=0;b<k;++b)
     {
         printf("%d",n[b]);
     }
    
    
    return 0;
  }
  

  
