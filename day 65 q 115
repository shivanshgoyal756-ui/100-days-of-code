#include <stdio.h>
#include <string.h>


int main() {
    char str[100];
    char find[100];
    int c=0;
    printf("Enter a string");
    scanf("%[^\n]",str);
    printf("Enter the 2nd string");
    scanf("%s",find);
    int len1=strlen(str);
    int len2=strlen(find);
    if(len1==len2)
    {
    for(int i=0;i<len1;++i)
    {
        for(int j=i;j<len1;++j)
        {
           if(str[i]==find[j])
           ++c;
        }
    }
    }
    if(c==len1+1)
    printf("Valid anagranm");
    else
    printf("Not valid anagram");
    return 0;
}
