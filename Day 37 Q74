#include <stdio.h>

int main() {
   int n,m;
   printf("Enter the row number and column number :");
   scanf("%d%d",&n,&m);
   int mat[n][m];
   for(int i=0;i<n;++i)
   {
       for(int j=0;j<m;++j)
       {
           printf("enter the number of %d row :",i+1);
           scanf("%d",&mat[i][j]);
       }
   }

for(int a=0;a<m;++a)
   {
       for(int b=0;b<n;++b)
       {
           printf(" %d",mat[b][a]);
          
       }
       printf("\n");
   }

    return 0;
}
