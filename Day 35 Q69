#include <stdio.h>

  int main() {
      int num;
      printf("Enter how many number are to be input 1st");
     scanf("%d",&num);
    int n[num];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
    for(int a=0;a<num-1;++a)
    {
        for(int b=0;b<num-a-1;b++)
        {
            if(n[b]>n[b+1])
            {
                int temp=n[b];
                n[b]=n[b+1];
                n[b+1]=temp;
            }
        }
    }
     
        printf("\n The second largest number is%d",n[num-2]);
    
    
    return 0;
  }
  

  
