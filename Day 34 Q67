#include <stdio.h>

int main() {
   int num;
   printf("Enter the size of the  array:");
   scanf("%d",&num);
   int a[num+1];
   for(int i=0;i<num;++i)
   {
      printf("Enter a number:");
      scanf("%d",&a[i]);
   }
   a[num]=0;
   int n;
   int k;
   printf("Enter the number placed");
   scanf("%d",&n);
   for(int p=0;p<num;++p)
   {
       if(a[p]>n)
       {
        k=p;
        break;
       }
   }
  
   for(int i=num-1;i>=k;--i)
   {
       a[i+1]=a[i];
   }
   a[k]=n;
   for(int j=0;j<=num;++j)
   {
       printf("\n%d",a[j]);
   }
    return 0;
}
