#include <stdio.h>

int main() {
   for(int i=1;i<=5;++i)
   {
       for(int j=1;j<i*2;++j)
       {
           printf("*");
       }
       printf("\n");
   }
   for(int a=4;a>=1;--a)
   {
       for(int b=1;b<a*2;++b)
       {
           printf("*");
       }
       printf("\n");
   }
    return 0;
}
