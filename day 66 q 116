#include <stdio.h>
#include <string.h>


int main() {
    int num;
    int c=0;
    printf("Enter how many numbers are to be inputed");
    scanf("%d",&num);
    int a[num];
    for(int i=0;i<num;++i)
    {
        printf("Enter the num :");
        scanf("%d",&a[i]);
    }
    int target;
    printf("Enter the target num");
    scanf("%d",&target);
    for(int i=0;i<num;++i)
    {
        for(int j=i;j<num;++j)
        {
            if(a[i]+a[j]==target)
            {
                printf("%d %d",i,j);
                c=1;
                break;
            }
            
        }
    }
    if(c==0)
    printf("-1 -1");
    return 0;
}
