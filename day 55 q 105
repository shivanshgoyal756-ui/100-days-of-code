#include <stdio.h>

int main() {
   int num;
   printf("Enter how many number are to be inputed");
   scanf("%d",&num);
   int a[num];
   int sum1=0;
   printf("\nEnter the array numbers");
   for(int i=0;i<num;++i)
   {
       scanf("%d",&a[i]);
   }
   int c=0;
   for(int i=0;i<num;++i)
   {
      for(int j=0;j<num;++j)
      {
          if(a[i]==a[j])
          sum1++;
      }
      if(sum1>num/2)
      {
          c=1;
          printf("\n%d",a[i]);
          break;
      }
      sum1=0;
   }
   if(c==0)
   printf("\n-1");
    return 0;
}
