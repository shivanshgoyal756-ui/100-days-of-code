#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    char str1[100];
    int c=0;
    printf("Enter a string 1 ");
    scanf("%[^\n]",str);
    printf("Enter the string 2");
    scanf("%s",str1);
    int l1=strlen(str);
    int l2=strlen(str1);
    if(l1==l2)
    {
    for(int i=0;i<l1-1;++i)
    {
       for(int j=0;j<l1-i-1;++j)
       {
           if((int)str[j]>(int)str[j+1])
           {
               char temp=str[j+1];
               str[j+1]=str[j];
               str[j]=temp;
           }
           if(str1[j]>str1[j+1])
           {
               char tem=str1[j+1];
               str1[j+1]=str1[j];
               str1[j]=tem;
           }
       }
    }
    
    for(int a=0;a<l1;++a)
    {
        if(str[a]==str1[a])
        ++c;
    }
    if(c==l1)
    printf("anagram");
    }
    else
    printf("not anagram");

    return 0;
}
