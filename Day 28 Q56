#include <stdio.h>

  int main() {
      int num;
    printf("Enter how many number are to be stored");
    scanf("%d",&num);
    int n[num];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
     for(int j=0;j<=num-1;++j)
    {
        printf("\n%d",n[j]);
    }
     
    return 0;
}
