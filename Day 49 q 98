#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    char str1[100];
    int c=0;
    printf("Enter a string 1 ");
    scanf("%[^\n]",str);
    int l1=strlen(str);
     int st=0;
     printf("%c.",str[0]);
    

    for(int i=0;i<l1;++i)
    {
    
        if(str[i]==' ')
        {  
          printf("%c .",str[i+1]);
          st=i+2;
        }
       
    }
    for(int i=st;i<l1;++i)
    {
        printf("%c",str[i]);
    }
    
    
    
    return 0;
}
    
    
