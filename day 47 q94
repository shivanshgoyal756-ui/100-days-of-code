#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    char str1[100];
    int c=0;
    printf("Enter a string 1 ");
    scanf("%[^\n]",str);
    int l1=strlen(str);
    int large=0;
    int st,end;
    st=0;
    int start;
    for(int i=0;i<l1;++i)
    {
        ++c;
        if(str[i]==' '||i==l1-1)
        {  

            if(large<c)
            {
            start=st;
            large=c;
            end=i;
            }
            st=i+1;
            c=0;
        }
       
    }
    
    for(int i=start;i<end+1;++i)
    {
        printf("%c",str[i]);
    }
    return 0;
}
    
    
