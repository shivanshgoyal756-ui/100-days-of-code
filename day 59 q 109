#include <stdio.h>

int main() {
   int num;
   int k=0;
   printf("Enter how many number are to be inputed");
   scanf("%d",&num);
   printf("Enter the value of k");
   scanf("%d",&k);
   int a[num];
   int sum1=0;
   int large=0;
   printf("\nEnter the array numbers");
   for(int i=0;i<num;++i)
   {
       scanf("%d",&a[i]);
   }
   for(int i=0;i<num;++i)
   {
      for(int j=i;j<i+k;++j)
      {
          sum1=sum1+a[j];
      }
     if(sum1>large)
     {
         large=sum1;
     }
     sum1=0;
   }
   printf("\n%d",large);
    return 0;
}
