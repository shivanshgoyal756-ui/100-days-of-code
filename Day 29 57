#include <stdio.h>

  int main() {
      int num;
      int sum=0;
    printf("Enter how many number are to be stored");
    scanf("%d",&num);
    int n[num];
    
     for(int i=1;i<=num;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
     for(int j=1;j<=num;++j)
    {
        sum=sum+n[j];
    }
     printf("the sum is %d",sum);
    return 0;
}
