#include <stdio.h>
void isprime(int num)
{
    int c=0;
    for(int i=2;i<=num/2;++i)
    {
        if(num%i==0)
        c=c+1;
    }
    if(c==0)
    printf("The num is prime");
    else 
    printf("The num is not prime");
    
    
}

int main() {
    int num;
    printf("Enter the number");
    scanf("%d",&num);
    isprime(num);
    return 0;
}
