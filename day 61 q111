#include <stdio.h>

int main() {
   int num;
   int k=0;
   printf("Enter how many number are to be inputed");
   scanf("%d",&num);
   printf("Enter the value of k");
   scanf("%d",&k);
   int a[num];
   int c=0;
   
   printf("\nEnter the array numbers");
   for(int i=0;i<num;++i)
   {
       scanf("%d",&a[i]);
   }
   for(int i=0;i<=num-k;++i)
   {
      for(int j=i;j<i+k;++j)
      {
          if(a[j]<0)
          {
          printf("%d ",a[j]);
          c=1;
          break;
          }
      }
      if(c==0)
      printf("0 ");
      c=0;
   }
  return 0;
}
