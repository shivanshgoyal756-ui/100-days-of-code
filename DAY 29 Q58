#include <stdio.h>

  int main() {
      int num;
      int sum=0;
    printf("Enter how many number are to be stored");
    scanf("%d",&num);
    int n[num];
    
     for(int i=1;i<=num;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
    int max=n[1];
    int min=n[1];
     for(int j=1;j<=num;++j)
    {
       if(n[j]>max)
       max=n[j];
       if(n[j]<min)
       min=n[j];
    }
     printf("the max is %d",max);
      printf("\nthe min is %d",min);
    return 0;
}
