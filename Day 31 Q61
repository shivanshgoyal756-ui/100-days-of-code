#include <stdio.h>
int main() {
      int num;
      int sum=0;
    printf("Enter how many number are to be stored");
    scanf("%d",&num);
    int n[num];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
    int ff=0;
    int c=0;
    printf("Enter the number to be searched");
    scanf("%d",&ff);
    
     for(int j=0;j<=num-1;++j)
    {
       if(n[j]==ff)
       {
           c=1;
           printf("Found at index: %d",j);
       }
       
    }
     if(c==0)
     printf("-1");
    return 0;
}
