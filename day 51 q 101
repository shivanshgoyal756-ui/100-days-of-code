#include <stdio.h>

int main() {
   int num;
   int ch;
   printf("\nEnter how many numbers are to placed in sorted array");
   scanf("%d",&ch);
   int a[ch+1];
   int c=0;
   for(int i=0;i<ch;++i)
   {
       scanf("%d",&a[i]);
   }
   printf("\nenter the target num");
   scanf("%d",&num);
   for(int i=0;i<ch;++i)
   {
      if(num==a[i])
      {
          printf("%d ,",i);
          c=1;
          break;
      }
   }
   for(int i=ch-1;i>=0;--i)
   {
      if(num==a[i])
      {
          printf("%d",i);
          break;
      }
   }
   if(c==0)
   printf(" -1 , -1");

    return 0;
}
