#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    char str1[100];
    int c=0;
    printf("Enter a string 1 ");
    scanf("%[^\n]",str);
    int l1=strlen(str);
     int st=0;
    

    for(int i=0;i<l1;++i)
    {
    
        if(str[i]==' ')
        {  
          st=i+1;
        }
       
    }
    printf("%c.%c.",str[0],str[st]);
    
    
    return 0;
}
    
    
