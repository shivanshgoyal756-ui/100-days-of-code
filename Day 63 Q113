#include <stdio.h>

int main() {
   int num;
   int k=0;
   printf("Enter how many number are to be inputed");
   scanf("%d",&num);
   printf("Enter the value of k");
   scanf("%d",&k);
   int a[num];
   printf("\nEnter the array numbers");
   for(int i=0;i<num;++i)
   {
       scanf("%d",&a[i]);
   }
  for(int i=0;i<num-1;++i)
  {
      for(int j=0;j<num-i-1;++i)
  {
      if(a[j]>a[j+1])
      {
          int temp=a[j];
          a[j]=a[j+1];
          a[j+1]=temp;
      }
  }
  }
  for(int i=0;i<num;++i)
  {
    if(i==k)
    printf("\n%d",a[k+1]);
  }
  
  return 0;
}
