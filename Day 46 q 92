#include <stdio.h>

int main() {
    char str[100];
    int c=0;
    printf("Enter a string ");
    scanf("%[^\n]",str);
    for(int i=0;str[i]!='\0';++i)
    {
       for(int j=i;str[j]!='\0';++j)
       {
           if(str[i]==str[j]&&str[i]>=97&&str[i]<=122)
           ++c;
           
       }
      if(c==2)
      {
          printf("%c",str[i]);
          break;
       }
       c=0;
    }

    return 0;
}
