#include <stdio.h>
int main() {
      int num;
    printf("Enter how many number are to be stored in 1 array");
    scanf("%d",&num);
    int n[num];
      int num2;
    printf("Enter how many number are to be stored in 2 array");
    scanf("%d",&num2);
    int nn[num2];
    int final[num+num2];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
        final[i]=n[i];
    }
    
    
     for(int z=0;z<=num2-1;++z)
    {
        printf("num");
        scanf("%d",&nn[z]);
        final[num+z]=nn[z];
    }

   for(int a=0;a<num+num2;++a)
   {
    printf("%d",final[a]);
   }
        
    return 0;
}
